{
  "ConnectionStrings": {
    "AppDb": "Data Source=data/db/TransmissionManager.db"
  },
  "Transmission": {
    "BaseAddress": "http://transmission:9091",
    "RpcEndpointAddressSuffix": "/transmission/rpc",
    "SessionHeaderName": "X-Transmission-Session-Id"
  },
  "TorrentWebPages": {
    "DefaultMagnetRegexPattern": "magnet:\\?xt=urn:btih:[^\"]+",
    "RegexMatchTimeout": "00:00:00.1"
  },
  "Cors": {
    "DefaultPolicy": {
      "Origins": [ "https://aannenko.github.io/transmission-manager" ],
      "Methods": [ "GET", "POST", "PATCH", "DELETE", "OPTIONS" ],
      "Headers": [ "*" ],
      "AllowCredentials": false
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "TransmissionManager": "Information"
    }
  },
  "AllowedHosts": "*",
  "http_ports": "9092"
}
